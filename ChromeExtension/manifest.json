{
    "manifest_version": 2,
  
    "name": "Phishing Detector",
    "description": "This extension will analyze a page using ML",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
        "tabs","<all_urls>","activeTab","webRequest"
     ],
     "background":
     {
         "scripts": ["jquery.min.js","background.js"]
     }
  }
  